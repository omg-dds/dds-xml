<?xml version="1.0" encoding="UTF-8"?>

<tsn xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xmlns="http://www.omg.org/spec/DDS-XML"
                    xsi:schemaLocation="http://www.omg.org/spec/DDS-XML ./dds-xml_configuration_tsn_definitions.xsd">
    <tsn_listener name="MyListener" stream_name="MyStream" datareader_ref="MyApp::MyParticipant::MyPub::MyDataReader">
        <network_requirements>
            <max_latency>3</max_latency>
            <num_seamless_trees>4</num_seamless_trees>
        </network_requirements>
    </tsn_listener>
    <tsn_talker name="MyTalker" stream_name="MyStream" datawriter_ref="MyApp::MyParticipant::MyPub::MyDataWriter">
        <traffic_specification>
            <max_bytes_per_sample>100</max_bytes_per_sample>
            <periodicity>
                <sec>0</sec>
                <nanosec>1000000</nanosec>
            </periodicity>
            <time_aware>
                <jitter>5</jitter>
                <earliest_transmit_offset>44</earliest_transmit_offset>
            </time_aware>
        </traffic_specification>
        <network_requirements>
            <max_latency>12</max_latency>
        </network_requirements>
        <data_frame_specification>
            <ipv4_tuple>
                <destination_ip_address>10.1.2.1</destination_ip_address>
                <protocol>3</protocol>
                <dscp>5</dscp>
                <source_port>55</source_port>
                <destination_port>88</destination_port>
                <source_ip_address>10.0.1.1</source_ip_address>
            </ipv4_tuple>
            <ipv6_tuple>
                <destination_ip_address>2001:0db8:85a3:0000:0000:8a2e:0370:1234</destination_ip_address>
                <source_ip_address>2001:0db8:85a3:0000:0000:8a2e:0370:5678</source_ip_address>
                <protocol>3</protocol>
                <dscp>5</dscp>
                <source_port>55</source_port>
                <destination_port>88</destination_port>
            </ipv6_tuple>
            <mac_addresses>
                <source_mac_address>AA:AA:AA:AA:AA:AA</source_mac_address>
                <destination_mac_address>BB:BB:BB:BB:BB:BB</destination_mac_address>
            </mac_addresses>
        </data_frame_specification>
    </tsn_talker>
</tsn>